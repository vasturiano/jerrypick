{
  "name": "jerrypick",
  "version": "1.0.0",
  "description": "Pluck and omit properties from a JS object",
  "unpkg": "dist/jerrypick.min.js",
  "main": "dist/jerrypick.common.js",
  "module": "dist/jerrypick.module.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/jerrypick.git"
  },
  "keywords": [
    "object",
    "properties",
    "pluck",
    "omit"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/jerrypick/issues"
  },
  "homepage": "https://github.com/vasturiano/jerrypick",
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c -w",
    "minify": "terser dist/jerrypick.js -o dist/jerrypick.min.js -c -m --comments '/Version/'",
    "build-module": "rollup -c rollup.config.module.js",
    "prepare": "npm run build && npm run minify && npm run build-module"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "terser": "^4.4.0"
  }
}
